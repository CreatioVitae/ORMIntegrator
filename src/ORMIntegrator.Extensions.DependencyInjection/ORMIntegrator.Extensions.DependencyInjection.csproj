<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.10.1.1</Version>
    <Company>Vitae</Company>
    <Copyright>Vitae</Copyright>
    <Product>ORMIntegrator.Extensions.DependencyInjection</Product>
    <Authors>Vitae</Authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <PackageId>ORMIntegrator.Extensions.DependencyInjection</PackageId>
    <Description>DI Extension For ORMIntegrator;</Description>
    <AssemblyName>ORMIntegrator.Extensions.DependencyInjection</AssemblyName>
    <RootNamespace>ORMIntegrator.Extensions.DependencyInjection</RootNamespace>
    <PackageProjectUrl>https://github.com/CreatioVitae/ORMIntegrator</PackageProjectUrl>
    <PackageReleaseNotes>Upgrade dotnet version.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BclExtensionPack.CoreLib" Version="0.14.1" />
    <PackageReference Include="ORMIntegrator" Version="0.10.1.1" />
    <PackageReference Include="SkiaSharp" Version="2.88.7" />
  </ItemGroup>
</Project>
