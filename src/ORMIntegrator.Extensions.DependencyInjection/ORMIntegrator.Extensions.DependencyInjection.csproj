    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.6.1.0</Version>
    <Company></Company>
    <Copyright></Copyright>
    <Product>ORMIntegrator.Extensions.DependencyInjection</Product>
